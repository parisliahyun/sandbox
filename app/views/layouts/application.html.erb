<!DOCTYPE html>
<html>
<head>
  <title>sandbox</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= tag :meta, name: "viewport", content: "width=device-width, initial-scale=1.0" %>
  <%= metamagic %>
</head>
<body>
  <%= render 'layouts/header' %>
<%= yield %>
  <div class="container">
    <%= render 'layouts/footer' %>
</div>
</body>
</html>
